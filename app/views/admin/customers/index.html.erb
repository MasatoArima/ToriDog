<div class="container">
  <div class="row">
    <div class="col-6">
      <h4>飼い主一覧</h4>
      <table class="table">
        <thead>
          <tr>
            <th>ID</th>
            <th>名前</th>
            <th>退会ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @dog_owners.each do |dog_owner| %>
            <tr>
              <td>
                <%= link_to admin_customer_path(dog_owner.id) do %>
                  <%= dog_owner.id %>
                <% end %>
              </td>
              <td><%= dog_owner.last_name %><%= dog_owner.first_name %></td>
              <td><%= dog_owner.is_deleted %></td>
            </tr>
        　<% end %>
        </tbody>
      </table>
    </div>
    <div class="col-6">
      <h4>トリマー一覧</h4>
      <table class="table">
        <thead>
          <tr>
            <th>ID</th>
            <th>名前</th>
            <th>退会ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @torimmers.each do |torimmer| %>
            <tr>
              <td>
                <%= link_to admin_customer_path(torimmer.id) do %>
                  <%= torimmer.id %>
                <% end %>
              </td>
              <td><%= torimmer.last_name %><%= torimmer.first_name %></td>
              <td><%= torimmer.is_deleted %></td>
            </tr>
        　<% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>





