<% if @contract.is_status == "in_preparation" %>
  <%= render 'customer/contracts/before_contract',dog_owner: @dog_owner,dog: @dog,trimmer: @trimmer, contract: @contract %>
<% else %>
  <%= render 'customer/contracts/after_contract',dog_owner: @dog_owner,dog: @dog,trimmer: @trimmer, contract: @contract %>
<% end %>